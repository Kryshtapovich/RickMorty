{
  "compilerOptions": {
    "allowJs": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "jsx": "react-native",
    "lib": ["ESNext"],
    "target": "ESNext",
    "moduleResolution": "node",
    "strict": true,
    "noEmit": true,
    "baseUrl": "./src",
    "paths": {
      "@hooks/*": ["./hooks/*"],
      "@models/*": ["./models/*"],
      "@services": ["./services"],
      "@services/*": ["./services/*"],
      "@context/*": ["./context/*"],
      "@components/*": ["./components/*"],
      "@screens/*": ["./screens/*"],
      "@navigation": ["./navigation"]
    }
  },
  "exclude": ["node_modules"]
}
